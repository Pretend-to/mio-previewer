{
  "name": "mio-previewer",
  "version": "0.0.0",
  "private": false,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build:lib": "vite build",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "pnpm run build:lib && pnpm run build:types",
    "preview": "vite preview",
    "benchmark": "node benchmark/runner.cjs",
    "prepare": "pnpm run build"
  },
  "dependencies": {
    "github-markdown-css": "^5.8.1",
    "htmlparser2": "^10.0.0",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "markdown-it-katex": "^2.0.3",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/htmlparser2": "^4.1.0",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-container": "^2.0.10",
    "@vitejs/plugin-vue": "^6.0.1",
    "puppeteer": "^20.7.4",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vue-tsc": "^1.1.0"
  },
  "main": "dist/mio-previewer.cjs.js",
  "module": "dist/mio-previewer.es.js",
  "types": "dist/types/src/index.d.ts",
  "peerDependencies": {
    "vue": "^3"
  },
  "description": "A small Vue 3 markdown previewer optimized for streaming updates",
  "keywords": [
    "markdown",
    "vue",
    "preview",
    "streaming"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "exports": {
    ".": {
      "import": "./dist/mio-previewer.es.js",
      "require": "./dist/mio-previewer.cjs.js"
    },
    "./package.json": "./package.json"
  }
}
